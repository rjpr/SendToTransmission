{
  "manifest_version": 2,
  "name": "Send to Transmission",
  "description": "Send torrent/magnet links to Transmission",
  "version": "1.1",
  "icons": {
    "16": "img/icon-16.png",
    "32": "img/icon-32.png",
    "48": "img/icon-48.png",
    "128": "img/icon-128.png"
  },

  "permissions": [
    "storage", 
    "contextMenus", 
    "<all_urls>"
  ],
  "background": {
    "scripts": ["js/background.min.js"]
  },
  "options_page": "options.html"
}